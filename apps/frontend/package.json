{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint src/ --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "tsc": "tsc",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "sync": "aws s3 sync dist/ s3://fodder.declanlscott.com/ --delete --exclude \".DS_Store\"",
    "invalidate": "aws cloudfront create-invalidation --distribution-id $(terraform -chdir=../../infra output -raw app_distribution_id) --paths \"/**/*\"",
    "deploy": "run-s sync invalidate"
  },
  "dependencies": {
    "@fodder/env": "workspace:*",
    "@fodder/schemas": "workspace:*",
    "@fodder/ui": "workspace:*",
    "@icons-pack/react-simple-icons": "^12.3.0",
    "@tanstack/react-query": "^5.70.0",
    "@tanstack/react-query-devtools": "^5.70.0",
    "@tanstack/react-router": "^1.114.29",
    "@tanstack/router-devtools": "^1.114.29",
    "@tanstack/router-vite-plugin": "^1.114.29",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.485.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "remeda": "^2.21.2",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "^1.0.0"
  },
  "devDependencies": {
    "@fodder/eslint-config": "workspace:*",
    "@fodder/prettier-config": "workspace:*",
    "@fodder/typescript-config": "workspace:*",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "autoprefixer": "^10.4.21",
    "fontaine": "^0.5.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "tailwindcss": "4.0.17",
    "typescript": "^5.8.2",
    "vite": "^6.2.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  },
  "prettier": "@fodder/prettier-config"
}
