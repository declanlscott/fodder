{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint src/ --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "tsc": "tsc",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "sync": "aws s3 sync dist/ s3://fodder.declanlscott.com/ --delete --exclude \".DS_Store\"",
    "invalidate": "aws cloudfront create-invalidation --distribution-id $(terraform -chdir=../../infra output -raw app_distribution_id) --paths \"/**/*\"",
    "deploy": "run-s sync invalidate"
  },
  "dependencies": {
    "@fodder/env": "workspace:*",
    "@fodder/schemas": "workspace:*",
    "@fodder/ui": "workspace:*",
    "@icons-pack/react-simple-icons": "^10.0.0",
    "@tanstack/react-query": "^5.59.6",
    "@tanstack/react-query-devtools": "^5.59.6",
    "@tanstack/react-router": "^1.63.5",
    "@tanstack/router-devtools": "^1.63.5",
    "@tanstack/router-vite-plugin": "^1.63.5",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.451.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "remeda": "^2.15.0",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "^0.42.1"
  },
  "devDependencies": {
    "@fodder/eslint-config": "workspace:*",
    "@fodder/prettier-config": "workspace:*",
    "@fodder/typescript-config": "workspace:*",
    "@tanstack/eslint-plugin-query": "^5.59.7",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "autoprefixer": "^10.4.20",
    "fontaine": "^0.5.0",
    "npm-run-all2": "^6.2.3",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.2"
  },
  "prettier": "@fodder/prettier-config"
}
