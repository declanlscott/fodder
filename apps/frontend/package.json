{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint src/ --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "sync": "aws s3 sync dist/ s3://fodder.declanlscott.com/ --delete --exclude \".DS_Store\"",
    "invalidate": "aws cloudfront create-invalidation --distribution-id $(terraform -chdir=../../infra output -raw app_distribution_id) --paths \"/**/*\"",
    "deploy": "run-s sync invalidate"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@icons-pack/react-simple-icons": "^9.4.0",
    "@repo/env": "workspace:*",
    "@repo/schemas": "workspace:*",
    "@repo/ui": "workspace:*",
    "@tanstack/react-query": "^5.28.4",
    "@tanstack/react-query-devtools": "^5.28.4",
    "@tanstack/react-router": "^1.20.1",
    "@tanstack/router-devtools": "^1.20.1",
    "date-fns": "^3.6.0",
    "ky": "^1.2.2",
    "lucide-react": "^0.359.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "remeda": "^1.53.0",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "^0.30.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/types": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tanstack/eslint-plugin-query": "^5.27.7",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.18",
    "fontaine": "^0.5.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.36",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  },
  "prettier": "@repo/prettier-config"
}
