{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint src/ --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "tsc": "tsc",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "sync": "aws s3 sync dist/ s3://fodder.declanlscott.com/ --delete --exclude \".DS_Store\"",
    "invalidate": "aws cloudfront create-invalidation --distribution-id $(terraform -chdir=../../infra output -raw app_distribution_id) --paths \"/**/*\"",
    "deploy": "run-s sync invalidate"
  },
  "dependencies": {
    "@fodder/env": "workspace:*",
    "@fodder/schemas": "workspace:*",
    "@fodder/ui": "workspace:*",
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-router": "^1.140.1",
    "@tanstack/router-devtools": "^1.140.1",
    "@tanstack/router-vite-plugin": "^1.140.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-error-boundary": "^6.0.0",
    "remeda": "^2.32.0",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@fodder/eslint-config": "workspace:*",
    "@fodder/prettier-config": "workspace:*",
    "@fodder/typescript-config": "workspace:*",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.23",
    "fontaine": "^0.7.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "tailwindcss": "4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.1",
    "vitest": "^4.0.15"
  },
  "prettier": "@fodder/prettier-config"
}
