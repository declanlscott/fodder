{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "sync": "aws s3 sync dist/ s3://fodder.declanlscott.com/ --delete --exclude \".DS_Store\"",
    "invalidate": "aws cloudfront create-invalidation --distribution-id $(terraform -chdir=../../infra output -raw app_distribution_id) --paths \"/**/*\"",
    "deploy": "run-s build sync invalidate"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@repo/env": "workspace:*",
    "@repo/ui": "workspace:*",
    "@tanstack/react-query": "^5.20.5",
    "@tanstack/react-query-devtools": "^5.21.0",
    "@tanstack/react-router": "^1.16.5",
    "@tanstack/router-devtools": "^1.16.5",
    "date-fns": "^3.3.1",
    "ky": "^1.2.0",
    "lucide-react": "^0.331.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "^0.28.1"
  },
  "devDependencies": {
    "@repo/eslintrc": "workspace:*",
    "@repo/prettierrc": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "prettier": "@repo/prettierrc"
}
