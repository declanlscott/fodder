{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint src/ --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "sync": "aws s3 sync dist/ s3://fodder.declanlscott.com/ --delete --exclude \".DS_Store\"",
    "invalidate": "aws cloudfront create-invalidation --distribution-id $(terraform -chdir=../../infra output -raw app_distribution_id) --paths \"/**/*\"",
    "deploy": "run-s sync invalidate"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@icons-pack/react-simple-icons": "^9.3.0",
    "@repo/env": "workspace:*",
    "@repo/ui": "workspace:*",
    "@tanstack/react-query": "^5.22.2",
    "@tanstack/react-query-devtools": "^5.24.0",
    "@tanstack/react-router": "^1.16.6",
    "@tanstack/router-devtools": "^1.16.6",
    "date-fns": "^3.3.1",
    "ky": "^1.2.0",
    "lucide-react": "^0.338.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "^7.50.1",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "^0.29.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/types": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@types/react": "^18.2.59",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.17",
    "fontaine": "^0.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1"
  },
  "prettier": "@repo/prettier-config"
}
